{
	"name": "ak-gemini",
	"author": "ak@mixpanel.com",
	"description": "AK's Generative AI Helper for doing... transforms",
	"version": "1.0.0",
	"main": "index.js",
	"files": [
		"index.js",
		"index.cjs",
		"types.ts",
		"logger.js"
	],
	"types": "types.ts",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ak--47/ak-gemini"
	},
	"bugs": {
		"url": "https://github.com/ak--47/ak-gemini/issues"
	},
	"homepage": "https://github.com/ak--47/ak-gemini#readme",
	"scripts": {
		"prepublishOnly": "npm run build:cjs",
		"publish": "npm publish --access public",
		"release": "npm version patch && npm publish --access public",
		"local": "./scripts/local.sh",
		"fire": "./scripts/fire.sh",
		"deploy": "./scripts/deploy.sh",
		"perms": "chmod +x ./scripts/*.sh",
		"update-deps": "npx npm-check-updates -u && npm install",
		"prune": "rm -rf tmp/*",
		"test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
		"build:cjs": "esbuild index.js --bundle --platform=node --format=cjs --outfile=index.cjs"
	},
	"type": "module",
	"keywords": [
		"gemini",
		"ai wrapper",
		"json transform"
	],
	"license": "ISC",
	"dependencies": {
		"@google-cloud/functions-framework": "^4.0.0",
		"@google/genai": "^1.3.0",
		"ak-tools": "^1.0.64",
		"dotenv": "^16.5.0",
		"pino": "^9.7.0"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"esbuild": "^0.25.5",
		"jest": "^29.7.0",
		"nodemon": "^3.1.10",
		"pino-pretty": "^13.0.0"
	}
}